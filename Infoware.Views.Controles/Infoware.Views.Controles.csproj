<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="BindingSourceGeneric.cs" />
    <Compile Remove="BindingSourceGeneric.Designer.cs" />
    <Compile Remove="MaintenanceBaseGUI.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="BindingSourceGeneric.resx" />
    <EmbeddedResource Remove="MaintenanceBaseGUI.resx" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Infoware.Core\Infoware.Core\Infoware.Core.csproj" />
    <ProjectReference Include="..\Infoware.Views\Infoware.Views.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="ComboBoxView.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
